{
  "name": "ARINO",
  "version": "1.1.1",
  "description": "A Nusic bot for Discord, written in Typescript using the Discord.js, JavaScript, Custom RyanLink (Lavalink) library.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "build": "tsc",
    "clean": "rm -rf dist",
    "postinstall": "npm run build",
    "prepare": "npm run build",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev --name init"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "lavalink",
    "arino",
    "ryanlink",
    "typescript"
  ],
  "author": "RY4N",
  "license": "GPL-3.0",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/sensible": "^6.0.3",
    "@prisma/client": "^6.13.0",
    "@top-gg/sdk": "^3.1.6",
    "@types/node": "^24.1.0",
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.11.0",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.1",
    "fastify": "^5.4.0",
    "fastify-plugin": "^5.0.1",
    "i18n": "^0.15.1",
    "install": "^0.13.0",
    "ioredis": "^5.7.0",
    "npm": "^11.5.2",
    "redis": "^5.8.1",
    "reflect-metadata": "^0.2.2",
    "signale": "^1.4.0",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "topgg-autoposter": "^2.0.2",
    "tslib": "^2.8.1",
    "tsyringe": "^4.10.0",
    "typescript": "^5.8.3",
    "undici": "^7.12.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.12",
    "@types/signale": "^1.4.7",
    "prisma": "^6.13.0"
  }
}
